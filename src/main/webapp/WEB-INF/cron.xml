<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
    <cron>
        <url>/persist_task</url>
        <description>Persist the datastore to the blobstore every day</description>
        <schedule>every 24 hours</schedule>
    </cron>
    <cron>
        <url>/porter</url>
        <description>Ports the blobstore to the new format</description>
        <schedule>every monday 09:00</schedule>
    </cron>
    <cron>
        <url>/update_cache</url>
        <description>Pushes new data into the mem cache</description>
        <schedule>every 14 minutes</schedule>
    </cron>
</cronentries>